      <ImageView fitHeight="95.0" fitWidth="95.0" layoutX="170.0" layoutY="56.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="/resources/image/java-icon.png" backgroundLoading="true"/>
